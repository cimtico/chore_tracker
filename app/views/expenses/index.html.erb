<p style="color: green"><%= notice %></p>

<h1>Expenses</h1>

<div id="expenses">
  <%= paginate @expenses %>
  <% @expenses.each do |expense| %>
    <%= render expense %>
    <p>
      <%= link_to "Show this expense", expense %>
    </p>
  <% end %>
  <%= paginate @expenses %>
</div>

<%= link_to "New expense", new_expense_path %>
<%= link_to "Back to chore entries", chore_entries_path %>
